#include <bits/stdc++.h> 
int largestElement(vector<int> &arr, int n) {
    
    int max = -1;
    for(int i=0 ; i<arr.size() ; i++)
    {
        if(arr[i]>max)
        {
            max=arr[i];
        }
    }
    // ans.push_back(max);
    return max;
}
